@model CarHub_Web.Models.VM.CarXFeatureUpdateVM<form asp-action="UpdateCarXFeature">    <br />    <div class="container border p-3">        <h4 class="text-primary">Update CarXFeature</h4>        <hr />        <input hidden asp-for="CarXFeature.Id" />        <div class="row">            <div asp-validation-summary="All" class="text-danger"></div>            <div class="col-2">                <label asp-for="CarXFeature.CarId" class="control-label pt-2" style="font-size:20px;"></label>            </div>            <div class="col-10 pb-3">                <select asp-for="CarXFeature.CarId" class="form-select" asp-items="@Model.CarList">                    <option disabled selected>--Select Car--</option>                </select>                <span asp-validation-for="CarXFeature.CarId" class="text-danger"></span>            </div>            <div class="col-2">                <label asp-for="CarXFeature.FeatureTypeId" class="control-label pt-2" style="font-size:20px;"></label>            </div>            <div class="col-10 pb-3">                <select asp-for="CarXFeature.FeatureTypeId" class="form-select" asp-items="@Model.FeatureTypeList">                    <option disabled selected>--Select FeatureType--</option>                </select>                <span asp-validation-for="CarXFeature.FeatureTypeId" class="text-danger"></span>            </div>            <div class="my-3">            </div>            <div class="py-2 col-12">                @for (int index = 0; index < @Model.Featurelist.Count(); index++)
                {                    <input type="checkbox" asp-for="@Model.Featurelist[index].IsChecked" class="form-check-input" />                    <label class="form-check-label" asp-for="@Model.Featurelist[index].IsChecked">@Model.Featurelist[index].Name</label>                    <input type="hidden" asp-for="@Model.Featurelist[index].Id" />                    <input type="hidden" asp-for="@Model.Featurelist[index].Name" />                    <br />                }            </div>            <div class="col-5 offset-2">                <input type="submit" value="Update" class="btn btn-primary form-control" />            </div>            <div class="col-5 ">                <a asp-action="IndexCarXFeature" asp-controller="CarXFeature" class="btn-primary btn form-control ">Back to List</a>            </div>                    </div>    </div></form>@section Scripts {    <partial name="_ValidationScriptsPartial" />}